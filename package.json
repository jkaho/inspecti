{
  "name": "inspecti",
  "version": "1.0.0",
  "description": "A real estate application for sharing property reviews and keeping track of personal property inspection information",
  "main": "server.js",
  "proxy": "http://localhost:3001",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkaho/inspecti.git"
  },
  "author": "Jessica Hong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkaho/inspecti/issues"
  },
  "homepage": "https://github.com/jkaho/inspecti#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "immutable": "^4.0.0-rc.12",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.6.2"
  }
}
