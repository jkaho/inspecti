{
  "name": "inspecti",
  "version": "1.0.0",
  "description": "A real estate application for sharing property reviews and keeping track of personal property inspection information",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkaho/inspecti.git"
  },
  "author": "Jessica Hong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkaho/inspecti/issues"
  },
  "homepage": "https://github.com/jkaho/inspecti#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.6",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  }
}
